{
    "version": 2,
    "framework": null,
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "outputDirectory": "public",
    "functions": {
        "api/**/*.php": {
            "runtime": "vercel-php@0.7.1",
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/build/$1"
        },
        {
            "src": "/css/(.*)",
            "dest": "/css/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/js/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/images/$1"
        },
        {
            "src": "/storage/(.*)",
            "dest": "/storage/$1"
        },
        {
            "src": "/favicon\\.(ico|png)",
            "dest": "/favicon.$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "DB_CONNECTION": "pgsql",
        "FILESYSTEM_DISK": "s3",
        "AWS_ACCESS_KEY_ID": "83fa20dbc49b20c9d9cb3c98f9c2f624",
        "AWS_SECRET_ACCESS_KEY": "22b93cee609b06c387e63cad9c0693d566153a9ff9fba05ae6807344403da4ec",
        "AWS_DEFAULT_REGION": "ap-southeast-1",
        "AWS_BUCKET": "products",
        "AWS_URL": "https://mxdbqetwltjmydkddjul.supabase.co/storage/v1/object/public/products",
        "AWS_ENDPOINT": "https://mxdbqetwltjmydkddjul.storage.supabase.co/storage/v1/s3",
        "AWS_USE_PATH_STYLE_ENDPOINT": "false",
        "LOG_CHANNEL": "stderr",
        "QUEUE_CONNECTION": "sync",
        "SESSION_DRIVER": "cookie",
        "CACHE_STORE": "array",
        "VIEW_COMPILED_PATH": "/tmp",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_SERVICES_CACHE": "/tmp/services.php"
    },
    "crons": [
        {
            "path": "/api/cron/cleanup-orders",
            "schedule": "0 1 * * *"
        },
        {
            "path": "/api/cron/sync-inventory",
            "schedule": "0 3 * * *"
        }
    ]
}